@model List<AboutViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Hakkımda</th>
        
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th> @item.AboutID</th>
            <td> @item.AboutText</td>
            
            <td> <a href="/About/UpdateAbout/@item.AboutID" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
        </tr>

    }
</table>
